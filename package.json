{
  "name": "wander-vr",
  "version": "1.0.0",
  "description": "Wander VR",
  "homepage": "https://xddtech.github.io/",
  "private": true,
  "scripts": {
    "clean": "rimraf dist aot",
    "start": "webpack-dev-server --inline --progress --port 8080",
    "test": "karma start karma.conf.js",
    "prebuild": "npm run clean",
    "build": "webpack --config webpack.prod.config.js --progress --profile --colors",
    "preserve:dist": "npm run build",
    "serve:dist": "static dist -H '{\"Cache-Control\": \"no-cache, must-revalidate\"}' -z",
    "prebuild:aot": "npm run clean",
    "build:aot": "ngc -p tsconfig.aot.json && webpack --config webpack.prod.config.aot.js --progress --profile --colors",
    "preserve:dist:aot": "npm run build:aot",
    "serve:dist:aot": "static dist -H '{\"Cache-Control\": \"no-cache, must-revalidate\"}' -z"
  },
  "dependencies": {
    "@types/three": "^0.84.35",
    "jquery": "^3.2.1",
    "three-js": "^79.0.0"
  },
  "devDependencies": {
    "@types/es6-shim": "^0.31.35",
    "@types/jasmine": "^2.8.2",
    "compression-webpack-plugin": "^1.1.3",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.7",
    "dedupe-plugin": "^1.0.1",
    "html-loader": "^0.5.1",
    "jasmine-core": "^2.8.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-webpack": "^2.0.9",
    "node-static": "^0.7.10",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.1",
    "to-string-loader": "^1.1.5",
    "ts-loader": "^3.2.0",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  }
}
